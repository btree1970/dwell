# Example environment variables for the rental scraper
# Copy this to .env and fill in your actual credentials

# Database settings
DATABASE_URL=sqlite:///./dwell.db

# Email client settings
EMAIL_USERNAME=your_email@example.com
EMAIL_PASSWORD=your_app_password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# OpenAI API settings
OPENAI_API_KEY=your_openai_api_key_here

# Listings Project credentials
LISTINGS_EMAIL=your_email@example.com
LISTINGS_PASSWORD=your_password

# Application settings
DEBUG=false
LOG_LEVEL=INFO
